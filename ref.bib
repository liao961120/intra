@article{diener2022,
  title = {Beyond {{Experiments}}},
  author = {Diener, Ed and Northcott, Robert and Zyphur, Michael J. and West, Stephen G.},
  date = {2022-07-01},
  journaltitle = {Perspectives on Psychological Science},
