@article{Axelrod1390,
  title = {The Evolution of Cooperation},
  author = {Axelrod, R and family=Hamilton, given=WD, given-i=WD},
  date = {1981},
  journaltitle = {Science},
  volume = {211},
  number = {4489},
  eprint = {http://science.sciencemag.org/content/211/4489/1390.full.pdf},
  pages = {1390--1396},
  publisher = {American Association for the Advancement of Science},
  issn = {0036-8075},
  doi = {10.1126/science.7466396},
  url = {http://science.sciencemag.org/content/211/4489/1390},
  abstract = {Cooperation in organisms, whether bacteria or primates, has been a difficulty for evolutionary theory since Darwin. On the assumption that interactions between pairs of individuals occur on a probabilistic basis, a model is developed based on the concept of an evolutionarily stable strategy in the context of the Prisoners Dilemma game. Deductions from the model, and the results of a computer tournament show how cooperation based on reciprocity can get started in an asocial world, can thrive while interacting with a wide range of other strategies, and can resist invasion once fully established. Potential applications include specific aspects of territoriality, mating, and disease.}
}

