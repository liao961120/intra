  title = {Can an Ape Create a Sentence?},
  author = {family=Terrace, given=HS, given-i=HS and family=Petitto, given=LA, given-i=LA and family=Sanders, given=RJ, given-i=RJ and family=Bever, given=TG, given-i=TG},
  date = {1979},
  journaltitle = {Science},
  volume = {206},
  number = {4421},
  eprint = {https://science.sciencemag.org/content/206/4421/891.full.pdf},
  pages = {891--902},
  publisher = {{American Association for the Advancement of Science}},
  issn = {0036-8075},
  doi = {10.1126/science.504995},
  url = {https://science.sciencemag.org/content/206/4421/891},
  abstract = {More than 19,000 multisign utterances of an infant chimpanzee (Nim) were analyzed for syntactic and semantic regularities. Lexical regularities were observed in the case of two-sign combinations: particular signs (for example, more) tended to occur in a particular position. These regularities could not be attributed to memorization or to position habits, suggesting that they were structurally constrained. That conclusion, however, was invalidated by videotape analyses, which showed that most of Nims utterances were prompted by his teachers prior utterance, and that Nim interrupted his teachers to a much larger extent than a child interrupts an adults speech. Signed utterances of other apes (as shown on films) revealed similar non-human patterns of discourse.}
}

