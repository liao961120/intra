@article{arch2022,
  title = {Acceptance and {{Commitment Therapy Processes}} and {{Mediation}}: {{Challenges}} and {{How}} to {{Address Them}}},
  author = {Arch, Joanna J. and Fishbein, Joel N. and Finkelstein, Lauren B. and Luoma, Jason B.},
